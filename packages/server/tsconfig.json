    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "webworker"],
    "outDir": "dist",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "preserveConstEnums": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src"]
